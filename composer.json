{
    "name": "ritaco/users",
    "type": "cakephp-plugin",
    "description": "This plugin is a set of tools for user handling like registration, login, email verification, password reset and much more.",
    "keywords": ["cakephp", "plugin", "users", "user-management", "login", "registration", "signup"],
    "homepage": "https://github.com/RitaCo/cakephp-users-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Saleh Souzanchi",
            "role": "Author",
            "homepage": "http://www.ritaco.net"
        }
    ],
    "require":{
        "php": ">=5.4",
        "cakephp/cakephp": "3.0.*-dev"
    },
    "autoload": {
        "psr-4": {
            "Rita\\Users\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rita\\Users\\Test\\": "tests"
        }
    },
    "support":{
        "source": "https://github.com/RitaCo/cakephp-users-plugin",
        "issues": "https://github.com/RitaCo/cakephp-users-plugin/issues"
    },
    "extra": {
        "installer-name": "Rita\Users"
    }
}
